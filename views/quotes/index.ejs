<%- include("../partials/header") %>

<% quotes.forEach((quote) => { %>
    <% if(quote.author) { %>
        <a href="/quotes/<%= quote._id %>/edit">
            <section>
                <h2>"<%= quote.content %>"</h2>
                <h5>Author: <%= quote.author.name %></h5>
            </section>
        </a>
    <% } else { %>
        <a href="/quotes/<%= quote._id %>/edit">
            <section>
                <h2>"<%= quote.content %>"</h2>
            </section>
        </a>
    <% } %>
<% }) %>

<%- include("../partials/footer") %>

