<%- include('../partials/header') %>
<h3>Here live your created quotes</h3>

<% quotes.forEach((quote) => { %>
    <section style="border: 2px solid black">
        <p>"<%= quote.content %>"</p>
        <p>Created: <%= quote.createdDate.toLocaleString("en-US", {timeZone: "UTC"}) %></p>
        <h6><%= quote.author %></h6>
        <button>Edit</button>
    </section>
    <% }) %>
<%- include('../partials/footer') %>